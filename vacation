import java.util.Scanner;
class Codechef1
{ 
	akash
	public static void main(String args[])
	{
		Scanner in=new Scanner(System.in);
		    int X = in.nextInt();
			int Y = in.nextInt();
			int Z = in.nextInt();
			if(X+Y<=Z)
			{
			System.out.println("Yes");
			}
			else
			{
			System.out.println("No");
			}
	}
}
		
